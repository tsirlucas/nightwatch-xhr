{
  "name": "protractor-xhr-plugin",
  "version": "0.1.1",
  "description": "A Protractor's XHR \"listener\" plugin for non-angular applications",
  "main": "lib/index.js",
  "scripts": {
	"prepare": "babel src --out-dir lib --copy-files --delete-dir-on-start",
	"build": "babel src --out-dir lib --copy-files --delete-dir-on-start",
    "build:watch": "babel src --out-dir lib --copy-files --delete-dir-on-start --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:verbose": "jest --watch --verbose",
    "coverage": "jest --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsirlucas/protractor-xhr-plugin.git"
  },
  "keywords": [
    "ajax",
    "xhr",
    "protractor"
  ],
  "author": "Lucas Correia <tsirlucas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsirlucas/protractor-xhr-plugin/issues"
  },
  "homepage": "https://github.com/tsirlucas/protractor-xhr-plugin#readme",
  "jest": {
    "testRegex": "test/([^/]+/?){2}\\.js$",
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "jest": "^22.0.1"
  }
}
